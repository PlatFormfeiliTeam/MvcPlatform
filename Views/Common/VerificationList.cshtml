@{
    ViewBag.Title = "VerificationList";
    Layout = "~/Views/Shared/_Layout.cshtml";
    string Version = System.Configuration.ConfigurationManager.AppSettings["Version"];
}
<link href="/js/Extjs42/resources/css/ext-all-gray.css" rel="stylesheet" type="text/css" />
<script src="/js/Extjs42/bootstrap.js" type="text/javascript"></script>
<link href="/css/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
<script src="~/js/pan.js?t=@Version"></script>
<script src="~/Views/Common/VerificationList.js?t=@Version"></script>
<style>
    .lab {
        font-weight: 100;
        color: #3071a9;
        top: 20px !important;
    }
</style>
<div class="container">
    <div id="div_form" style="width:100%;"></div>
    <div>
        <div class="btn-group" role="group">
            <button onclick="importfile('add')" type="button" class="btn btn-primary btn-sm"><i class="fa fa-upload"></i>&nbsp;新增导入</button>
            <button onclick="importfile('update')" type="button" class="btn btn-primary btn-sm"><i class="fa fa-edit"></i>&nbsp;更新导入</button>
        </div>
        <div class="btn-group fr" role="group">
            <button onclick="Select()" class="btn btn-primary btn-sm"><i class="fa fa-search"></i>&nbsp;查询</button>
            <button onclick="Reset()" type="button" class="btn btn-primary btn-sm"><i class="fa fa-refresh"></i>&nbsp;重置</button>
        </div>
    </div>
    <div id="appConId" style="width:100%"></div>
</div>

