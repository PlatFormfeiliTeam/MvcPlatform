@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    string Version = System.Configuration.ConfigurationManager.AppSettings["Version"];
}

<link href="/js/Extjs42/resources/css/ext-all-gray.css" rel="stylesheet" type="text/css" />
<script src="/js/Extjs42/bootstrap.js" type="text/javascript"></script>
<link href="/css/font-awesome/css/font-awesome.min.css" rel="stylesheet" />

<script src="/js/commondata.js?t=@Version"></script>
<script src="/js/pan.js?t=@Version" type="text/javascript"></script>
<script src="/Views/OrderManager/index.js?t=@Version" type="text/javascript"></script>

<div class="container">
    <div id="div_form" style="width:100%;height:70px"></div>
    <div>
        <div class="btn-group" role="group">
            <button id="btn_open" onclick="Open()" type="button" class="btn btn-primary btn-sm"><i class="fa fa-plus fa-fw"></i>&nbsp;添加</button>
            <button onclick="EditOrderM()" type="button" class="btn btn-primary btn-sm"><i class="fa fa-pencil-square-o fa-fw"></i>&nbsp;修改</button>
            <button onclick="CompleteOrderM()" type="button" class="btn btn-primary btn-sm"><i class="fa fa-check"></i>&nbsp;业务完成</button>
            <button onclick="DeleteOrderM()" type="button" class="btn btn-primary btn-sm"><i class="fa fa-trash-o"></i>&nbsp;删除</button>
        </div>
        <div class="btn-group fr" role="group">
            <button onclick="changeOnlySelfStyle()" class="btn btn-primary btn-sm"><i id="OnlySelfi" class="fa fa-check-square-o"></i>&nbsp;仅本人</button>
            <button onclick="Select()" class="btn btn-primary btn-sm"><i class="fa fa-search"></i>&nbsp;查询</button>
            <button onclick="seniorOrderM('pgbar')" class="btn btn-primary btn-sm"><i class="fa fa-search"></i>&nbsp;高级查询</button>
            <button onclick="Reset()" type="button" class="btn btn-primary btn-sm"><i class="fa fa-refresh"></i>&nbsp;重置</button>
            <form id="exportform" name="form" enctype="multipart/form-data" method="post" style="display:inline-block">
                <button type="button" id="btn_Export" class="btn btn-primary btn-sm" onclick="ExportOrderM()"><i class="fa fa-level-down"></i>&nbsp;导出</button>
            </form>
        </div>
    </div>
    <div id="appConId" style="width:100%"></div>
</div> 
